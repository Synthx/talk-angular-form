{
    "singleQuote": true,
    "printWidth": 120,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "tabWidth": 4,
    "plugins": ["prettier-plugin-organize-attributes", "prettier-plugin-organize-imports"],
    "attributeGroups": [
        "$DEFAULT",
        "$ANGULAR_INPUT",
        "$ANGULAR_TWO_WAY_BINDING",
        "$ANGULAR_OUTPUT",
        "$ANGULAR_STRUCTURAL_DIRECTIVE"
    ],
    "attributeSort": "ASC"
}
